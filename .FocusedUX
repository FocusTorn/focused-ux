FocusedUX:
  info_message_show_seconds: 2
ContextCherryPicker:
  settings:
    message_show_seconds: 3
  quick_settings_panel:
    project_structure_contents:
      visible: true
      filter: selected
    default_status_message:
      visible: false
      style: drop
    file_groups:
      config:
        initially_visible: true
        items:
          - '**/.FocusedUX'
          - '**/.prettierignore'
          - '**/eslint.config.js'
          - '**/.npmrc'
          - '**/knip*'
          - '**/prettier.config.cjs'
          - '**/.cursor-rules*'
          - '**/.vscode-test.mjs'
      build:
        initially_visible: false
        items:
          - '**/**/dist'
          - '**/**/out-tsc'
          - '**/*.tsbuildinfo'
          - '**/.turbo'
          - '**/.nx'
          - '**/tmp'
      test:
        initially_visible: false
        items:
          - '**/__tests__'
          - '**/coverage'
      ignore:
        initially_visible: false
        items:
          - '**/.gitignore'
          - '**/.vscodeignore'
          - '**/.npmignore'
          - '**/.turbignore'
      docs:
        initially_visible: true
        items:
          - '**/LICENSE.*'
          - '**/README.*'
      locks:
        initially_visible: false
        items:
          - '**/pnpm-lock.yaml'
          - '**/package-lock.json'
  ignore:
    - '**/node_modules'
    - .git
    - '**/workspace-tools'
    - _removed
    - assets
    - vsix_packages
    - '**/*.png'
    - '**/*.jpg'
    - '**/*.jpeg'
    - '**/*.svg'
    - '**/*.bak*'
    - '**/.vscode-test'
  project_tree:
    always_show: null
    always_hide: null
    show_if_selected: null
  context_explorer:
    ignore: null
    hide_children:
      - '**/tests/integration/test-user-data'
TerminalButler:
  packageJson-order:
    - displayName
    - description
    - publisher
    - repository
    - version
    - bin
    - scripts
    - sideEffects
    - dependencies
    - devDependencies
    - peerDependencies
    - contributes
    - activationEvents
    - license
    - icon
    - categories
    - keywords
    - engines
    - private
    - type
    - module
    - main
    - types
    - workspaces
    - exports
ProjectButler:
  packageJson-order:
    - displayName
    - description
    - publisher
    - repository
    - version
    - bin
    - scripts
    - sideEffects
    - dependencies
    - devDependencies
    - peerDependencies
    - contributes
    - activationEvents
    - license
    - icon
    - categories
    - keywords
    - engines
    - private
    - type
    - module
    - main
    - types
    - workspaces
    - exports
