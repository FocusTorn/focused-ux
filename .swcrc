{
    "jsc": {
        "target": "es2022",
        "parser": {
            "syntax": "typescript",
            "decorators": true,
            "dynamicImport": true
        },
        "transform": {
            "decoratorMetadata": true,
            "legacyDecorator": true
        },
        "keepClassNames": true,
        "externalHelpers": true,
        "loose": true,
        "minify": {
            "mangle": false,
            "compress": false
        }
    },
    "module": {
        "type": "nodenext"
    },
    "sourceMaps": true,
    "exclude": [".*\\.spec.tsx?$", ".*\\.test.tsx?$", ".*\\.vite.tsx?$", ".*.js$"]
}
