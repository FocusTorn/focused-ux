{
    "name": "ai-agent-interactor",
    "displayName": "F-UX: AI Agent Interactor",
    "description": "Automates the transfer of code from an AI Agent into a VSCode file.",
    "publisher": "NewRealityDesigns",
    "repository": "https://github.com/FocusTorn/focused-ux",
    "version": "0.1.0",
    "private": true,
    "main": "./dist/extension.cjs",
    "engines": {
        "vscode": "^1.100.0"
    },
    "dependencies": {
        "@fux/ai-agent-interactor-core": "workspace:*",
        "@fux/utilities": "workspace:*",
        "awilix": "^12.0.5"
    },
    "activationEvents": [
        "onUri"
    ],
    "contributes": {
        "uriHandler": [
            {
                "id": "transfer",
                "label": "F-UX AI Agent Code Transfer"
            }
        ],
        "configuration": {
            "title": "F-UX: AI Agent Interactor",
            "properties": {
                "fux-ai-agent-interactor.projectBase": {
                    "type": "string",
                    "default": "",
                    "description": "The absolute base path of the project to which relative file paths from the AI will be resolved."
                }
            }
        }
    }
}
