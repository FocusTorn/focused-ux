{
    "name": "@fux/shared-services",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        },
        "./node": {
            "types": "./dist/node.d.ts",
            "import": "./dist/node.js"
        },
        "./*": {
            "types": "./dist/*.d.ts",
            "import": "./dist/*.js"
        }
    },
    "dependencies": {
        "strip-json-comments": "^5.0.2",
        "tsyringe": "^4.10.0"
    }
}
